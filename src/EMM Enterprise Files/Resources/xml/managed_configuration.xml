<?xml version="1.0" encoding="utf-8" ?>
<restrictions xmlns:android="http://schemas.android.com/apk/res/android">

  <restriction
    android:description="@string/managed_config_profiles_array_description"
    android:key="profiles"
    android:restrictionType="bundle_array"
    android:title="@string/managed_config_profiles_array_title">

    <restriction
    android:description="@string/managed_config_profile_bundle_description"
    android:key="profile"
    android:restrictionType="bundle"
    android:title="@string/managed_config_profile_bundle_title">
      
    <restriction
        android:defaultValue="false"
        android:description="@string/managed_config_name_description"
        android:key="name"
        android:restrictionType="string"
        android:title="@string/managed_config_name_title" />

    <restriction
        android:defaultValue="false"
        android:description="@string/managed_config_url_description"
        android:key="url"
        android:restrictionType="string"
        android:title="@string/managed_config_url_title" />

    <restriction
      android:defaultValue="false"
      android:description="@string/managed_config_path_description"
      android:key="path"
      android:restrictionType="string"
      android:title="@string/managed_config_path_title" />
      <restriction
  android:defaultValue="false"
  android:description="@string/managed_config_intent_description"
  android:entryValues="@array/managed_config_intent_values"
  android:entries="@array/intent_values"
  android:key="intent"
  android:restrictionType="choice"
  android:title="@string/managed_config_intent_title" />
      <restriction
  android:defaultValue="false"
  android:description="@string/managed_config_hash_description"
  android:key="hash"
  android:restrictionType="string"
  android:title="@string/managed_config_hash_title" />
    
  </restriction>
    
  
    </restriction>
</restrictions>